{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "ESNext",
      "dom"
    ],
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "resolveJsonModule": true,
//    "noUnusedLocals": true,
//    "strictNullChecks": false,
//    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false
  },
  "include": [
    "scripts",
    ".vitepress/plugins",
    ".vitepress/theme"
  ]
}